{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "types": [
      "node"
    ]
  },
  "files": [
    "index.ts",
    "lib/icon-picker.module.ts",
    "lib/icon-picker.service.ts",
    "lib/icon-picker.component.ts",
    "lib/icon-picker.directive.ts"
  ],
  "angularCompilerOptions": {
    "skipMetadataEmit": false,
    "skipTemplateCodegen": true
  },
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true
  }
}
